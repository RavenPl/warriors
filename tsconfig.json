{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es6",
    "downlevelIteration": true,
    "lib": [
      "es6",
      "dom.iterable",
      "DOM",
      "dom"
    ],
    "outDir": "dist",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "Node",
    "esModuleInterop": true
  }
}
